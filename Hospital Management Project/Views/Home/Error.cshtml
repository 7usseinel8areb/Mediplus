@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}

<section class="error-page section">
			<div class="container">
				<div class="row">
					<div class="col-lg-6 offset-lg-3 col-12">
						<!-- Error Inner -->
						<div class="error-inner">
							<h1>@Model.StatusCode<span>@ViewData["ErrorMessage"]</span></h1>
					<p>If you believe this is an error or need further assistance, please contact support or your system administrator.</p>
							<form class="search-form">
								<input placeholder="Search from Here" type="text">
								<button class="btn" type="submit"><i class="fa fa-search"></i></button>
							</form>
						</div>
						<!--/ End Error Inner -->
					</div>
				</div>
			</div>
</section>	